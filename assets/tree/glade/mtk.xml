<?xml version="1.0" encoding="UTF-8"?>
<glade-catalog name="gfiletree" library="mtk" depends="webkit2gtk,gtksourceview-4">

  <glade-widget-classes>
    <glade-widget-class nick="FileTree"  get-type-function="mtk_filetree_get_type" name="MtkFiletree" generic-name="filetree" title="File Tree" >
    </glade-widget-class>
    <glade-widget-class  name="MtkWebView" get-type-function="mtk_webview_get_type" generic-name="webview" title="Web View" >
    </glade-widget-class>    
    <glade-widget-class  name="MtkEditor" get-type-function="mtk_editor_get_type" generic-name="editor" title="Editor" >
    </glade-widget-class>    
  </glade-widget-classes>
  
  <glade-widget-group name="mtk" title="MTK">
    <glade-widget-class-ref name="MtkFiletree"/>
    <glade-widget-class-ref name="MtkWebView"/>
    <glade-widget-class-ref name="MtkEditor"/>
  </glade-widget-group>  

</glade-catalog>
